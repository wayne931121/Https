[req]
distinguished_name = req_distinguished_name
x509_extensions = v3_req
prompt = no
[req_distinguished_name]
C = US
L = SomeCity
O = MyCompany
OU = MyDivision
CN = AAA mydomain
[v3_req]
keyUsage = critical, digitalSignature, keyAgreement
extendedKeyUsage = serverAuth
subjectAltName = DNS:mydomain, IP:127.0.0.1